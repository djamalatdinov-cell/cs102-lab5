#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;
int main() {
    srand(time(0));
    int even = 0, odd = 0;
    for (int i = 0; i < 100000; i++) {
        int x = rand();
        if (x % 2 == 0) even++;
        else odd++;
    }
    cout << "Even: " << even << "\nOdd: " << odd;

return 0;
}
